{
    "manifest_version": 3,
    "name": "Amazon Wishlist Total",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "extension-icon.png"
    },
    "permissions": [
        "tabs",
        "scripting"
    ],
    "host_permissions": [
        "https://www.amazon.com/*"
    ]
}